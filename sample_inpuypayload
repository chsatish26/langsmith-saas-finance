Python code 

import requests, json
BASE_URL = "https://<your-domain>"
API_KEY = "<if-required>"

payload = {
  "loans": [
    {"borrower_id":"b1","loan_balance":400000,"current_rate":0.065,"remaining_term_months":300},
    {"borrower_id":"b2","loan_balance":280000,"current_rate":0.072,"remaining_term_months":240}
  ],
  "rates": [
    {"date":"2025-08-13","term_months":360,"rate":0.058},
    {"date":"2025-08-13","term_months":240,"rate":0.055},
    {"date":"2025-08-13","term_months":300,"rate":0.057}
  ],
  "market_rate": 0.0,
  "savings": [],
  "alerts": []
}

r = requests.post(
    f"{BASE_URL}/graphs/mortgage-refi/invoke",
    headers={"Content-Type":"application/json","Authorization":f"Bearer {API_KEY}"},
    data=json.dumps(payload)
)
print(r.status_code, r.text)


